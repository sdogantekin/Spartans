<% include header %>
<body>

<div class = "container">
    <div class="wrapper">
        <form action="/signup" method="post" role="form" id="signupForm">
            <h3>Please Signup</h3><br>
            <input type="text" name="username" id="username" placeholder="Username" required="true" autofocus="" oninvalid="this.setCustomValidity('you must fill username')"/><br>
            <input type="password" name="password" id="password" placeholder="Password" required="true"/><br>
            <button id="signupButton" name="Submit" value="Signup" type="Submit">Signup</button>
        </form>
    </div>
</div>
</body>
<% include footer %>